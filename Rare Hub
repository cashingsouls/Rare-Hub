-- \\ Universal Scripts // --

Tab:Button{
	Name = "CMD X",
	Description = nil,
	Callback = function() 
        local x = os.clock()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source", true))()
        local t = (string.format("%."..tostring(decimals).."f", os.clock() - x))
        GUI:Notification{
            Title = "Rare Hub",
            Text = "Loaded CMD X In:\n" .. t .. " Seconds!",
            Duration = 3,
            Callback = function() end
        }
    end
}

