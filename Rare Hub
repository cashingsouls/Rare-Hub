-- \\ Misc // --

Tab4:Button{
	Name = "Headless (R6)",
	Description = nil,
	Callback = function() 
        game:GetService('Players').LocalPlayer.Character:WaitForChild('Head').Mesh.MeshId = "134079402"
    end
}

Tab4:Button{
	Name = "Korblox (R6)",
	Description = nil,
	Callback = function() 
        local KorbloxLeg = Instance.new('CharacterMesh')
        KorbloxLeg.Parent = game:GetService('Players').LocalPlayer.Character
        KorbloxLeg.BodyPart = Enum.BodyPart.RightLeg
        KorbloxLeg.MeshId = '101851696'
        KorbloxLeg.OverlayTextureId = '101851254'
    end
}

Tab4:Button{
	Name = "Forcefield (R6)",
	Description = nil,
	Callback = function() 
        local Player = game:GetService('Players').LocalPlayer.Character

        Player.Head.Material = Enum.Material.ForceField
        Player.Torso.Material = Enum.Material.ForceField
        Player:WaitForChild('Right Arm').Material = Enum.Material.ForceField
        Player:WaitForChild('Left Arm').Material = Enum.Material.ForceField
        Player:WaitForChild('Right Leg').Material = Enum.Material.ForceField
        Player:WaitForChild('Left Leg').Material = Enum.Material.ForceField
    end
}

Tab4:Button{
	Name = "Reset",
	Description = nil,
	Callback = function() 
        local Player = game:GetService("Players").LocalPlayer
        local Character = Player.Character
        local Humanoid = Character:FindFirstChild("Humanoid")
        local Torso = Character:FindFirstChild("Torso")
        
        function ResetCharacter()
            if game.Players.LocalPlayer.Character then
                Torso:Destroy()
            else
                Humanoid:ChangeState(Enum.HumanoidStateType.Running)
                Humanoid.Health = 0
            end
        end
        
        ResetCharacter()
    end
}

Tab4:Toggle{
	Name = "Noclip",
	StartingState = false,
	Description = nil,
	Callback = function(state) 
        getgenv().canGoThru = state
    end
}

local plyr = game:GetService("Players").LocalPlayer
game:GetService("RunService").Stepped:Connect(function()
    if getgenv().canGoThru == true then
        for i,v in pairs(plyr.Character:GetDescendants()) do
            if v:IsA("BasePart") and v.CanCollide == true then
                v.CanCollide = false
            end
        end
    end
end)

Tab4:Slider{
	Name = "Walkspeed",
	Default = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed,
	Min = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed,
	Max = 10000,
	Callback = function(Value) 
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end
}

Tab4:Slider{
	Name = "JumpPower",
	Default = 50,
	Min = 50,
	Max = 10000,
	Callback = function(Value) 
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
    end
}

